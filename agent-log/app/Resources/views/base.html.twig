<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Percona Agent Log{% endblock %}</title>
        {% block stylesheets %}
            <link href="{{ asset('/public/css/bootstrap.css') }}" rel="stylesheet" />
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <div class="container">
            {% block body %}{% endblock %}
        </div>
        {% block javascripts %}
            <script src="{{ asset('/public/js/bootstrap.js') }}"></script>
        {% endblock %}
    </body>
</html>
